{
  "name": "supra-api-nodejs",
  "version": "0.0.0",
  "private": true,
  "author": "Sasha Zmts",
  "scripts": {
    "start:dev": "NODE_ENV=development node ./main.js",
    "start:dev-watch": "NODE_ENV=development nodemon ./main.js",
    "start:prod": "NODE_ENV=production node ./main.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "test": "echo \">>> Keep in mind: to enshure right test you should set seed data and run server :) <<<\" && mocha test/*.test.js",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@sentry/node": "6.15.0",
    "aws-sdk": "2.1040.0",
    "bcryptjs": "2.4.3",
    "dotenv": "10.0.0",
    "jsonwebtoken": "8.5.1",
    "knex": "2.4.0",
    "mailgun-js": "0.22.0",
    "ms": "2.1.3",
    "objection": "3.0.0",
    "pg": "8.7.1",
    "redis": "4.0.0",
    "stack-trace": "0.0.10",
    "stdout-stream": "1.4.1",
    "supra-core": "./core",
    "uuid": "8.3.2",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "eslint": "8.3.0",
    "husky": "7.0.4",
    "mocha": "9.1.3",
    "standart": "6.1.0",
    "supertest": "6.1.6"
  }
}
